{
  "rules": {
    "align-column-to-the-first": "warn",
    "column-new-line": "warn",
    "linebreak-after-clause-keyword": "off",
    "reserved-word-case": ["error", "upper"],
    "space-surrounding-operators": "warn",
    "where-clause-new-line": "warn",
    "align-where-clause-to-the-first": "warn",
    "require-as-to-rename-column": 0,
  }
}
