{
    "configurations": {
        "launch": {
            "adapter": "CodeLLDB",
            "filetypes": [
                "rust"
            ],
            "configuration": {
                "request": "launch",
                "program": "${workspaceRoot}/target/debug/vimspector_test"
            }
        },
        "attach": {
            "adapter": "CodeLLDB",
            "filetypes": [
                "rust",
                "c",
                "cpp",
                "jai"
            ],
            "configuration": {
                "request": "attach",
                "program": "${workspaceRoot}/${fileBasenameNoExtension}",
                "PID": "${PID}"
            }
        }
    }
}
